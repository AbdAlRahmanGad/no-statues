{
  "manifest_version": 3,
  "name": "Remove Whatsapp Web Status Button",
  "description": "Simple extension to remove Whatsapp Web Status Button.",
  "version": "1.0",
  "icons": {
    "16": "/Icons/icon-16.png",
    "48": "/Icons/icon-48.png",
    "128": "/Icons/icon-128.png"
  },
  "content_scripts": [{
    "matches": ["https://web.whatsapp.com/"],
    "js": ["/remove-wpp-status.js"]
  }]
}